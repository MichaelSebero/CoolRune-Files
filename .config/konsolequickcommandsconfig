[-  Commands -][Admin File Manager]
command=sudo pcmanfm
name=Admin File Manager
tooltip=

[-  Commands -][Authentication Log Settings]
command=doas covermyass
name=Authentication Log Settings
tooltip=

[-  Commands -][Change MAC Address]
command=sudo macchanger wlan0 -r\nsudo macchanger enp0s3 -r\nsudo macchanger eth0 -r\nsudo macchanger eth1 -r\nsudo macchanger lo -r
name=Change MAC Address
tooltip=

[-  Commands -][Clear Cache]
command=doas pacman -Sc\s
name=Clear Cache
tooltip=

[-  Commands -][Clear Command History]
command=history -c
name=Clear Command History
tooltip=

[-  Commands -][Fix Corruption]
command=doas pacman -Syu --overwrite "*" $(pacman  -Qnq)
name=Fix Corruption
tooltip=

[-  Commands -][Fix Keyrings]
command=doas -Sy archlinux-keyring\ndoas -Sy artix-keyring
name=Fix Keyrings
tooltip=

[-  Commands -][Fix Pamac]
command=sudo mkdir -p  /var/lib/pacman/sync/ &&\npamac update --force-refresh &&\n\nsudo rm /var/tmp/pamac/dbs/db.lck &&\nsudo rm -r /var/lib/pacman/sync/ &&\nsudo pacman -Sy &&\npamac update --no-refresh\n
name=Fix Pamac
tooltip=

[-  Commands -][List Packages]
command=pacman -Qe
